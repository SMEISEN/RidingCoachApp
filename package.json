    "lodash": "^4.17.21",
    "mathjs": "^9.1.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "node-sass": "^4.14.1",
    "roboto-fontface": "*",
    "serialize-javascript": "^4.0.0",
    "vue": "^2.6.12",
    "vue-chartjs": "^3.5.1",
    "vue-clickaway": "^2.2.2",
    "vue-directive-long-press": "^1.1.0",
    "vue-lodash": "^2.1.2",
    "vue-meta": "^2.4.0",
