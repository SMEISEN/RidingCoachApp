  },
  "main": "frontend/src/main.js",
  "dependencies": {
    "@mdi/font": "^3.9.97",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.4",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-sass": "^4.14.1",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-clickaway": "^2.2.2",
    "vue-directive-long-press": "^1.1.0",
    "vue-lodash": "^2.1.2",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.4",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^3.0.1",
    "websocket-driver": "^0.7.4",
    "websocket-extensions": "^0.1.4"
