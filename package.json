{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "frontend/src/main.js",
  "scripts": {
    "serve": "cd frontend && vue-cli-service serve",
    "build": "cd frontend && vue-cli-service build",
    "lint": "cd frontend && vue-cli-service lint",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "12.16.x"
  },
  "dependencies": {
    "@mdi/font": "^3.9.97",
    "roboto-fontface": "*",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "node-sass": "^4.14.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
